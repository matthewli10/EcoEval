{
    "manifest_version": 3,
    "name": "Hello World",
    "description": "A simple Hello World Chrome Extension",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "permissions": [
      "activeTab", "scripting", "tabs"
    ],
    "host_permissions": [
      "<all_urls>"
    ]
}
