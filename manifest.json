{
    "manifest_version": 3,
    "name": "GreenGrade",
    "description": "A Chrome Extension that allows you to be eco-friendly!",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "permissions": [
      "activeTab"
    ],
    "host_permissions": [
      "<all_urls>"
    ]


  }
  